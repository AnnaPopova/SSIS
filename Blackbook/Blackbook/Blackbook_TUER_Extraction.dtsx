        <DTS:Executable
          DTS:refId="Package\Preparation\Get file local folder location"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{C1919FD8-3308-4AB6-91BC-66FA7419DC34}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Get file local folder location"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2014; © 2014 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="2">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{9F4FCCA1-6C90-4B01-9E3F-35A47827326C}"
              SQLTask:SqlStatementSource="  select E as source_location,T as destination_location from&#xA;  (&#xA;  select p.Process_Type,ds.Source_Location&#xA;  from Processes as p&#xA;  inner join Processes_Hierarchy as ph on ph.Child_Process=p.Process_ID&#xA;  inner join Data_Sources as ds on p.Process_ID=ds.Process_ID&#xA;  where p.Process_Type in ('E','T')&#xA;  and ph.Parent_Process=6&#xA;   and p.Process_Name like'%TUER%'&#xA;  &#xA;  ) as s&#xA;  pivot (&#xA;  MAX(source_location) for process_type in (E,T)&#xA;  ) as pvt"
              SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ResultBinding
                SQLTask:ResultName="destination_location"
                SQLTask:DtsVariableName="User::DestinationFolder" />
            </SQLTask:SqlTaskData>
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Preparation\Get object name"
